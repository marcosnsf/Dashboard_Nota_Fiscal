<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
<script type=" text/javascript" src="/public/js/materialize.min.js"></script>
<script>
    var token = localStorage.getItem('token');
    document.addEventListener('DOMContentLoaded', function () {
        var elems = document.querySelectorAll('.sidenav');
        var instances = M.Sidenav.init(elems, null);
    });

    getServicos()

    async function getServicos() {
        var dados = `
             <thead>
                <tr>
                    <th>Emissão Nota Fiscal</th>
                    <th>N° Nota Fiscal</th>
                    <th>N° Empenho</th>
                    <th>N° pedido Nota Fiscal</th>
                    <th>N° Contrato:</th>
                    <th>Recebimento Nota Fiscal:</th>
                    <th>Valor da nota:</th>
                    <th>Arquivo Nota Fiscal</th>
                    <th>Arquivo Laudo</th>
                    <th>Secretaria</th>
                    <th>Fornecedor</th>
                    <th>Pedido de compra</th>
                    <th>Situação e Aprovação</th>
                    <th>Ultimo operador da Nota</th>
                    <th>Observações</th>
                    <th>Editar</th>
                    <th>Remover</th>
                </tr>
            </thead>
            <tbody>
        `
        await axios.get('/api/servicos', { headers: { authorization: `Bearer ${token}` } })
            .then(data => {
                data.data.forEach(servico => {
                    dados = dados + `
                        <tr>
                            <td>${servico.datanf1}</td>
                            <td>${servico.numnf1}</td>
                            <td>${servico.numemp1}</td>
                            <td>${servico.numpedido1}</td>
                            <td>${servico.numcontrato1}</td>
                            <td>${servico.datareceb1}</td>
                            <td>${servico.valorVenda1.toLocaleString('pt-br',{style: 'currency', currency: 'BRL'})}</td>
                            <td>${servico.arquivonf}</td>
                            <td>${servico.arquivolaudo}</td>
                            <td>${servico.secretaria} | ${servico.gestor}</td>
                            <td>${servico.fornecedorRazaoSocial} | ${servico.fornecedorCnpj.replace(/^(\d{2})(\d{3})(\d{3})(\d{4})(\d{2})/, "$1.$2.$3/$4-$5")}</td>
                            <td>${servico.pedidocompra1}</td>
                            <td>${servico.situacao} | ${servico.aprovacao}</td>
                             <td>${servico.operador1} | ${servico.dtmodificacao1}</td>
                             <td>${servico.observation1}</td>
                            <td>
                                <button class="btn waves-effect waves-light"
                                    onclick="window.location.href='/produtos/cadastrarServicos/${servico._id}';">
                                    <i class="material-icons">edit</i></button>
                            </td>
                            <td>
                                <button class="btn-flat waves-effect waves-light red"
                                    onclick="removerServico('${servico._id}')">
                                    <i class="material-icons">delete</i></button>
                            </td>
                        </tr>
                    `
                })
                dados = dados + `</tbody>`
                document.getElementById('table').innerHTML = dados
            })
            .catch(error => {
                console.log(error) 
                document.getElementById('error').innerHTML = error.response.data.message
            })
        document.getElementById('loading').style.display = "none";
    }

    document.getElementById('loading').style.display = "none";

    async function removerServico(id) {
        document.getElementById('loading').style.display = "block";
        await axios.delete(`/api/servicos/${id}`, { headers: { authorization: `Bearer ${token}` } })
            .then(data => {
                location.reload();
            })
            .catch(error => {
                document.getElementById('error').innerHTML = error.response.data.message
            })
        document.getElementById('loading').style.display = "none";
    }
</script>

